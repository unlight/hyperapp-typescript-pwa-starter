{
  "name": "hyperapp-typescript-pwa-starter",
  "version": "0.0.0-d",
  "license": "MIT",
  "description": "Template for hyperapp projects",
  "main": "dist/index.js",
  "typings": "src/index.ts",
  "scripts": {
    "dev": "npm run server",
    "server": "webpack-dev-server --hot --colors",
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:w": "jest --watch",
    "test:r": "jest --runInBand --verbose",
    "test:cov": "npm run test:r -- --collectCoverage",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --testMatch \"<rootDir>/src/**/*.spec.ts\" \"<rootDir>/src/**/*.spec.tsx\"",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint src --ext ts",
    "eslint:fix": "eslint src --ext \"ts\" --fix",
    "eslint:w": "chokidar \"src/**/*.ts\" --initial -c \"npm run eslint\"",
    "lint:w": "run-p tsclint:w eslint:w",
    "build:libs": "npm run build -- --env.libs --progress",
    "build": "webpack",
    "build:prod": "webpack --mode=production",
    "clean": "rimraf dist",
    "commit": "git-cz"
  },
  "dependencies": {
    "@hyperapp/router": "0.7.0",
    "hyperapp": "1.2.6",
    "tslib": "1.9.2"
  },
  "devDependencies": {
    "1-liners": "0.4.0",
    "@types/execa": "0.9.0",
    "@types/jest": "23.0.0",
    "@types/mocha": "5.2.1",
    "@types/node": "10.3.1",
    "@types/webpack": "4.4.0",
    "@types/webpack-env": "1.13.6",
    "add-asset-html-webpack-plugin": "2.1.3",
    "autoprefixer": "8.6.0",
    "chokidar-cli": "1.2.0",
    "copy-webpack-plugin": "4.5.1",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-tslint": "2.1.0",
    "eslint-plugin-typescript": "0.12.0",
    "eslint-plugin-unicorn": "4.0.3",
    "execa": "0.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.1.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "script-ext-html-webpack-plugin": "2.0.1",
    "source-map-loader": "0.2.3",
    "ts-jest": "22.4.6",
    "ts-loader": "4.3.1",
    "ts-node": "5.0.1",
    "tslint": "5.10.0",
    "typescript": "2.9.1",
    "typescript-eslint-parser": "16.0.0",
    "webpack": "4.11.0",
    "webpack-cli": "3.0.2",
    "webpack-dev-server": "3.1.4",
    "webpack-loader-helper": "1.0.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "keywords": []
}
